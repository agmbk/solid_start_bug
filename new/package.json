{
	"scripts": {
		"build": "vinxi build",
		"deps:audit": "pnpm audit --fix",
		"deps:check": "depcheck --ignore-patterns=dist,public,.solid",
		"deps:update": "pnpm update -i --latest",
		"dev": "vinxi dev --host",
		"docs": "typedoc",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"lint:css": "stylelint \"src/**/*.{css,scss}\" --fix",
		"start": "vinxi start",
		"types-check": "tsc --noEmit --skipLibCheck --pretty --project tsconfig.json",
		"types-sync": "typeshare .. --lang=typescript --output-file=src/api-types.ts"
	},
	"devDependencies": {
		"@solid-primitives/event-listener": "^2.3.3",
		"@solid-primitives/input-mask": "^0.2.2",
		"@solid-primitives/lifecycle": "^0.0.102",
		"@solid-primitives/rootless": "^1.4.5",
		"@solid-primitives/scheduled": "^1.4.3",
		"@solid-primitives/utils": "^6.2.3",
		"@solidjs/meta": "^0.29.3",
		"@solidjs/router": "^0.13.1",
		"@solidjs/start": "^1.0.0-rc.0",
		"@types/gtag.js": "^0.0.19",
		"@types/node": "^20.12.6",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"autoprefixer": "^10.4.19",
		"cssnano": "^6.1.2",
		"depcheck": "^1.4.7",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-perfectionist": "^2.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-solid": "^0.13.2",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"sass": "^1.74.1",
		"solid-js": "^1.8.16",
		"stylelint": "^16.3.1",
		"stylelint-config-recess-order": "^5.0.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"stylelint-order": "^6.0.4",
		"typedoc": "^0.25.12",
		"typescript": "^5.4.4",
		"vinxi": "^0.3.11",
		"vite": "^5.2.8",
		"vite-plugin-solid-svg": "^0.8.1"
	},
	"engines": {
		"node": "~20.12.2",
		"npm": "use pnpm",
		"yarn": "use pnpm",
		"pnpm": "~8.15.6"
	},
	"type": "module"
}
